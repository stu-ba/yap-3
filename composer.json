{
    "name": "stu-ba/yap",
    "description": "Ytrium Admin Panel is integration for Taiga and GitHub.",
    "keywords": [
        "ytrium",
        "project-management",
	"stu-ba"
    ],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Martin Kiesel",
            "email": "martin.kiesel@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "5.4.*",
        "kyslik/django-signer": "0.0.*",
        "kyslik/column-sortable": "5.4.*",
        "laravel/socialite": "3.0.*",
        "graham-campbell/github": "5.1.*",
        "php-http/guzzle6-adapter": "^1.1",
        "madewithlove/illuminate-psr-cache-bridge": "1.0.*",
        "erusev/parsedown-extra": "0.7.*",
        "league/commonmark": "0.15.*",
        "symfony/dom-crawler": "3.2.*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "2.3.*",
        "barryvdh/laravel-ide-helper": "2.3.*",
        "doctrine/dbal": "~2.3",
        "fzaninotto/faker": "~1.4",
        "laravel/browser-kit-testing": "^1.0",
        "laravel/tinker": "~1.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "spatie/laravel-tail": "^1.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Yap\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
